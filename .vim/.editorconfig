;.editorconfig

root = true

[**.js]
path=~/.vim/bundle/js-beautify/js/lib/beautify.js
indent_style = space
bin = node
indent_size = 2

[**.json]
indent_style = space
indent_size = 2

[**.jsx]
e4x = true
indent_style = space
indent_size = 2

[**.css]
indent_style = space
indent_size = 2

[**.html]
vim:max_char = 78:brace_style=expand
indent_style = space
indent_size = 2
